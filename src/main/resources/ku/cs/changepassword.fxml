            <Font size="14.0" />
         </font></TextField>
      <PasswordField fx:id="newPasswordField" layoutX="321.0" layoutY="258.0" prefHeight="43.0" prefWidth="208.0" promptText="new password" style="-fx-background-color: #ffffff;">
